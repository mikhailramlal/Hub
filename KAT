TextButton_2.Size = UDim2.new(0, 172, 0, 32)
TextButton_2.Font = Enum.Font.SourceSansBold
TextButton_2.Text = "V.G Hub"
TextButton_2.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton_2.TextSize = 16.000
